{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --files src/index.ts",
    "build": "tsc && cp ./package.json ./dist",
    "build:lambda": "tsc",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/database.config.ts",
    "db:generate": "npm run typeorm -- migration:generate ./src/database/migrations/UpdateMeeting_Table",
    "db:migrate": "npm run typeorm -- migration:run",
    "db:revert": "npm run typeorm -- migration:revert",
    "db:drop": "npm run typeorm -- schema:drop",
    "db:setup": "node scripts/setup-database.js",
    "db:setup-supabase": "node scripts/setup-supabase.js",
    "db:update-supabase": "node scripts/update-supabase-env.js",
    "deploy": "npm run build:lambda && serverless deploy",
    "deploy:production": "npm run build:lambda && serverless deploy --stage production",
    "deploy:staging": "npm run build:lambda && serverless deploy --stage staging",
    "test:lambda": "serverless offline",
    "remove": "serverless remove",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@vendia/serverless-express": "^4.12.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "google-auth-library": "^9.0.0",
    "googleapis": "^144.0.0",
    "js-base64": "^3.7.7",
    "multer": "^2.0.2",
    "openai": "^6.14.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@serverless/typescript": "^4.30.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.13.4",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@vitest/ui": "^2.1.8",
    "serverless": "^3.39.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.8.1",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-webpack": "^5.15.3",
    "ts-loader": "^9.5.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "webpack": "^5.104.1",
    "webpack-node-externals": "^3.0.0",
    "zod": "^3.24.1"
  }
}
